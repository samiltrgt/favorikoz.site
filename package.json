{
  "name": "favorikoz-site",
  "version": "1.0.0",
  "description": "Modern e-ticaret sitesi - Next.js, TypeScript ve Tailwind CSS ile geliştirilmiştir",
  "private": true,
  "author": "Favorikoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/favorikoz-site.git"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "tailwindcss",
    "ecommerce",
    "react",
    "vercel"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "import:excel": "node scripts/import-excel.js",
    "migrate:products": "tsx scripts/migrate-to-supabase.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest --testPathPattern=api",
    "test:components": "jest --testPathPattern=components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.0",
    "iyzipay": "^2.0.64",
    "lucide-react": "^0.294.0",
    "next": "14.0.0",
    "next-cloudinary": "^6.16.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/iyzipay": "^2.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/xlsx": "^0.0.35",
    "autoprefixer": "^10.0.1",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20",
    "xlsx": "^0.18.5"
  }
}
